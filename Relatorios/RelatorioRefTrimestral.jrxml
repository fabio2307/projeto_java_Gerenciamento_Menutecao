	<queryString>
		<![CDATA[select Men.id_refrigeracao_trimestral as 'N°', Usu.iduser as 'R.E', Usu.nome_usuario as 'Funcionario', Men.tipo_usuario as 'Especialidade', Men.tipo_prev as 'Preventiva', Men.situacao_prev as 'Situação', Men.tempo_ini_prev as 'inicío', Men.tempo_total_prev as 'Tempo', Men.tipo_prev as 'Tipo',
(select count(*)  from form_refrigeracao_trimestral where situacao_prev is null) as 'Iniciar',
(select count(*)  from form_refrigeracao_trimestral where situacao_prev = "Aberta") as 'Aberta',
(select count(*)  from form_refrigeracao_trimestral where situacao_prev = "Encerrada") as 'Encerrada',
(select count(*)  from form_refrigeracao_trimestral where situacao_prev = "irregular") as 'irregular'
from form_refrigeracao_trimestral Men
inner join usuarios Usu on  Men.iduser = Usu.iduser
group by(Usu.nome_usuario), (Men.id_refrigeracao_trimestral)
order by Funcionario asc;]]>
	</queryString>
